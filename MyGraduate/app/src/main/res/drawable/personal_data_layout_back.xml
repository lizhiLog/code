<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--描边-->
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#66969696" />
        </shape>
    </item>

    <!--填充背景色-->
    <item
        android:bottom="1dp"
        android:top="1dp">
        <shape>
            <solid android:color="#ffffff" />
        </shape>
    </item>
</layer-list>